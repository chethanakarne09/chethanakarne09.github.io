<script>
function startTime(){
	document.getElementById('clock').innerHTML = new Date();
	setTimeOut(startTime,500);
}

</script>
<script>

var shown=false;
function showhide(){

	if(shown)
	{
		document.getElementById('email').innerHTML = "show my email";
		shown=false;
	}
	else
	{
		var myemail="<a href='mailto: Chethana" + "@" + "udayton.edu'>chethana" + "@" + "udayton.edu</a>";
		document.getElementById('email').innerHTML = "myemail";
		shown=true;
	}

}



</script>

